/dts-v1/;
#include <st/f3/stm32f303Xc.dtsi>

/ {
	model = "Alturia Altimeter";
	compatible = "alturia";

	chosen {
		zephyr,console = &usart1;
		zephyr,shell-uart = &usart1;
		zephyr,sram = &sram0;
		zephyr,flash = &flash0;
	};

	leds {
		compatible = "gpio-leds";
		user_led: led_0 {
			gpios = <&gpiof 0 GPIO_INT_ACTIVE_HIGH>;
			label = "User LED";
		};
	};

	aliases {
		led0 = &user_led;
	};
};

&usart1 {
	current-speed = <115200>;
	pinctrl-0 = <&usart1_pins_b>;
	pinctrl-names = "default";
	status = "okay";
};

&timers2 {
	status = "okay";
	pwm {
		st,prescaler = <100>;
		status = "okay";
	};
};

&spi1 {
	stauts = "okay";
};

&adc1 {
	status = "okay";
};

&usb {
	status = "okay";
};
