/dts-v1/;
#include <st/f3/stm32f303Xc.dtsi>

/ {
	model = "Alturia Altimeter";
	compatible = "alturia";

	chosen {
		zephyr,console = &usart1;
		zephyr,sram = &sram0;
		zephyr,flash = &flash0;
	};

	leds {
		compatible = "gpio-leds";
		user_led: led_0 {
			gpios = <&gpiof 0 GPIO_INT_ACTIVE_HIGH>;
			label = "User LED";
		};
	};
};

&usart1 {
	current-speed = <115200>;
	pinctrl-0 = <&usart1_pins_d>;
	pinctrl-names = "default";
	status = "okay";
};

&spi1 {
	stauts = "okay";
};

&spi2 {
	status = "okay";
};

&usb {
	status = "okay";
};
